<h1>You created a contact!</h1>
<ul>
  @foreach (var contact in Model)
  {
    <li>Name: @contact.GetName()</li>
    <li>Address: @contact.GetAddress()</li>
    <li>Phone Number: @contact.GetPhoneNumber()</li>
    <br>
  }
</ul>
<p><a href='/'>Return home</a></p>
<p><a href='/contact/new'>Add another contact</a></p>
<p><a href='/all_contacts'>View all contacts</a></p>
